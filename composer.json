{
    "name": "kangaroorewards/oauth2-kangaroo-rewards",
    "description": "Kangaroo Rewards OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Ursuleac",
            "email": "ursuleacv@gmail.com",
            "homepage": "http://valnet.ca",
            "role": "Developer"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "Kangaroo Rewards",
        "Kangaroo",
        "loyalty"
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "league/oauth2-client": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "KangarooRewards\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KangarooRewards\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
